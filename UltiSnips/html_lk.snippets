snippet lk_calander "Loankit calander"

        <div class="row">
          <span class="label">Date</span>
          <input name="dtDate" _class="dtpicker hasDatepicker" type="text" value="05/05/2014" id="adddate" class="hasDatepicker">
          <input type="hidden" name="todo_dd" value="05">
          <input type="hidden" name="todo_mm" value="05">
          <input type="hidden" name="todo_yyyy" value="2014">
        </div>

        <script>
          $(document).ready(function () {
              $( "#adddate" ).datepicker({
                    dateFormat: 'dd/mm/yy',
                      onSelect: function() { $(this).change(); }
                });

              $( "#adddate" ).change( function() {
                var dt = $.datepicker.parseDate( "dd/mm/yy", $( this ).val() );
                var dd = $.datepicker.formatDate( "dd", dt );
                var mm = $.datepicker.formatDate( "mm", dt );
                var yy = $.datepicker.formatDate( "yy", dt );

                $("input[name='todo_dd']").val( dd );
                $("input[name='todo_mm']").val( mm );
                $("input[name='todo_yyyy']").val( yy );
              });
          });
        </script>

endsnippet

snippet lk_tabs "Loankit tabs"

      <div id="section-tabs" class="ui-tabs ui-widget ui-widget-content ui-corner-all ui-tabs-collapsible">
          <ul class="ui-tabs-nav ui-helper-reset ui-helper-clearfix ui-widget-header ui-corner-all" role="tablist">
              <li class="ui-state-default ui-corner-top" role="tab" tabindex="-1" aria-controls="tab-related-parties" aria-labelledby="ui-id-6" aria-selected="false"><a href="#tab-related-parties" class="ui-tabs-anchor" role="presentation" tabindex="-1" id="ui-id-6">Related Parties</a></li>
              <li class="ui-state-default ui-corner-top" role="tab" tabindex="-1" aria-controls="tab-attachments" aria-labelledby="ui-id-7" aria-selected="false"><a href="#tab-attachments" class="ui-tabs-anchor" role="presentation" tabindex="-1" id="ui-id-7">Attachments</a></li>
              <li class="ui-state-default ui-corner-top" role="tab" tabindex="0" aria-controls="tab-all" aria-labelledby="ui-id-9" aria-selected="false"><a href="#tab-all" class="btn-view-all ui-tabs-anchor" role="presentation" tabindex="-1" id="ui-id-9">View All</a></li>
          </ul>

          <div id="tab-attachments" class="clearfix ui-tabs-panel ui-widget-content ui-corner-bottom" aria-labelledby="ui-id-7" role="tabpanel" aria-expanded="false" aria-hidden="true" style="display: none;">
              <header>
                  <h2 style="margin-left:10px;">Attachments</h2>
              </header>
              <div style="padding:10px;"></div>
          </div>
          <div id="tab-compliance" style="padding-bottom: 1px; display: none;" aria-labelledby="ui-id-8" class="ui-tabs-panel ui-widget-content ui-corner-bottom" role="tabpanel" aria-expanded="false" aria-hidden="true"></div>
      </div>


    <script type="text/javascript">
        $(document).ready(function () {
            $('div#section-tabs').tabs({
                collapsible:true
            });

            $('#section-tabs > ul > li > a.ui-tabs-anchor').on('click', function() {
                var targetPanel = $(this).attr('href');
                hideAllTabs();
                showTab(targetPanel);

            });

            $('a.btn-view-all').on('click', function () {
                openAllTabs();
            });

        });

        function showTab(tabId) {
            $('div' + tabId).css('display', 'block');
            $('div' + tabId).attr('aria-expanded', true);
            $('div' + tabId).attr('aria-hidden', false);
        }

        function openAllTabs() {
            $("div[id^='tab-']").css('display', 'block');
            $("div[id^='tab-']").attr('aria-expanded', true);
            $("div[id^='tab-']").attr('aria-hidden', false);
        }

        function hideAllTabs() {
            $("div[id^='tab-']").css('display', 'none');
            $("div[id^='tab-']").attr('aria-expanded', false);
            $("div[id^='tab-']").attr('aria-hidden', true);
        }
    </script>

endsnippet

snippet lk_form "Loankit form"

      <section id="form-info">
        <header>
          <h2>Compare Splits</h2>
        </header>
        <div>
